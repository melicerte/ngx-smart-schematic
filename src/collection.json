{
  "$schema": "../node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "openapi": {
      "description": "Generates Angular Services and Data Transfer Objects to user an API",
      "factory": "./openapi",
      "schema": "./openapi/schema.json"
    },
    "http-service": {
      "description": "Services generator",
      "factory": "./http-service",
      "schema": "./http-service/schema.json"
    },
    "models": {
      "description": "Data Transfer Objects generator",
      "factory": "./models",
      "schema": "./models/schema.json"
    }
  }
}
